}
@using stockTable.Models;
@model Equipment

<h1>Информация об оборудование</h1>
<hr>

<h3>Тип оборудования: @Model.TypeEq</h3>
<h3>Модель оборудования: @Model.Model</h3>
<h3>Имя ПК: @Model.NamePC</h3>
<h3>Операционая система: @Model.OCName</h3>
<h3>IP адрес: @Model.IPAddress</h3>
<h3>Статус обородувание: @Model.Status.Name</h3>
<h3>Цена: @Model.Price</h3>
